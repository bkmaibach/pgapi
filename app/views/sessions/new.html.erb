<h1>Log In</h1>

<p id="notice"><%= notice %></p>

<%= form_with(scope: :session, url: login_path) do |form| %>
  
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>


  <div class="actions">
    <%= form.submit("Log in") %>
  </div>
<% end %>
